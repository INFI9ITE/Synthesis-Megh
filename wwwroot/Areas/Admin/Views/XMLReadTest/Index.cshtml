
@model IEnumerable<wwwroot.Areas.Admin.Models.CustomerModel>

@{
    Layout = null;
}


<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>Customer Id</th>
            <th>Name</th>
            <th>Country</th>
        </tr>
     
                @foreach (wwwroot.Areas.Admin.Models.CustomerModel customer in Model)
                {
                    <tr>
                        <td>@customer.CustomerId</td>
                        <td>@customer.Name</td>
                        <td>@customer.Country</td>
                    </tr>
                }
            
    </table>
</body>
</html>